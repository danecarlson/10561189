


public class Possibles {
	int x, y;
	String word;
	Input.Direction dir;
	
	public Possibles(){
		x = 0;
		y = 0;
		word = "";
		dir = null;
	}
	
	public Possibles(String wordp, int xp, int yp){
		x = xp;

		System.out.println("This is being hit this many times");
		y = yp;
		word = wordp;
		dir = null;
	}
	
	public void setDirection(Input.Direction dirp){
		
		dir = dirp;	
		
	}
	
	public String getWord(){
		return word;
	}

}
